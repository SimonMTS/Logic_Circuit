<Window x:Class="Logic_Circuit.ResultWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Logic_Circuit"
        mc:Ignorable="d"
        WindowState="Maximized"
        Loaded="Window_Loaded"
        Title="ResultWindow" Height="450" Width="800">
    <Grid>
        <ScrollViewer Margin="0,0,0,0">
            <Canvas Name="Canvas" Width="10" Height="10" HorizontalAlignment="Left" VerticalAlignment="Top"></Canvas>
        </ScrollViewer>
    </Grid>
</Window>
